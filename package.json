{
  "name": "ember-frost-notifier",
  "version": "8.0.0",
  "description": "An ember service and component for managing application notifications",
  "frostGuideDirectory": "ui-components/indicator-controls/notifications",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "check-coverage": "pr-bumper check-coverage",
    "lint": "lint-all-the-things",
    "start": "ember server",
    "test": "npm run lint && npm run test-addon",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=ember-default} && ember try:one $EMBER_TRY_SCENARIO --- COVERAGE=true ember test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ciena-frost/ember-frost-notifier.git"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": "Sophy Pal (https://github.com/sophypal)",
  "contributors": [
    "Roxanne Panthaky (https://github.com/rox163)",
    "Steven Glanzer (https://github.com/sglanzer)",
    "David Fortin (https://github.com/dafortin)",
    "Matthew Dahl (https://github.com/sandersky)"
  ],
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.8.2",
    "broccoli-asset-rev": "^2.4.5",
    "chai-jquery": "^2.0.0",
    "ember-cli": "2.12.3",
    "ember-cli-chai": "0.4.3",
    "ember-cli-code-coverage": "0.3.12",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-frost-blueprints": "^5.0.2",
    "ember-cli-htmlbars-inline-precompile": "0.3.12",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "0.14.4",
    "ember-cli-shims": "^1.0.2",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-frost-test": "^4.0.1",
    "ember-load-initializers": "^0.6.0",
    "ember-resolver": "^2.0.3",
    "ember-sinon": "^0.7.0",
    "ember-source": "~2.12.0",
    "ember-test-utils": "^8.1.1",
    "loader.js": "^4.2.3",
    "pr-bumper": "^3.7.0",
    "sinon-chai": "^2.14.0"
  },
  "keywords": [
    "ember-addon",
    "frost"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.1.7",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-sass": "7.1.1",
    "ember-computed-decorators": "0.3.0",
    "ember-frost-core": "^7.0.0",
    "ember-hook": "1.4.2"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "ember-frost-icon-pack": {
    "name": "frost-notifier"
  },
  "pr-bumper": {
    "coverage": 88.24
  }
}